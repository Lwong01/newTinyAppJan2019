<!DOCTYPE html>
<html lang="en">

<head>
  <title>TinyApp</title>
</head>

<body>
  <% include partials/_header %>
  <table width="30%" border="1">
    <% for (let url in urls) { %>
    <tr>
      <td><%= url %></td>

      <td><%= urls[url] %></td>


      <td>
        <form method="POST" action="/urls/<%= url %>/delete"">
        <input type="submit" value="Delete">
      </form>
      </td>


    </tr>

    <% } %>

  </table>




</body>

</html>